\mode<presentation>

\definecolor{LUHblue}{RGB}{0,80,155}
\definecolor{LUHgreen}{RGB}{200,211,23}
\definecolor{LUHgraytwenty}{RGB}{203,203,203}

% Settings

\setbeamercolor*{structure}{fg=LUHblue}

\setbeamercolor*{palette primary}{fg=LUHblue,bg=gray!10}
\setbeamercolor*{palette primary}{fg=LUHblue,bg=}
\setbeamercolor*{palette secondary}{fg=white,bg=LUHblue}
\setbeamercolor*{palette tertiary}{fg=black,bg=LUHgraytwenty}
\setbeamercolor*{palette quaternary}{fg=white,bg=LUHgreen}

\setbeamercolor*{author in head/foot}{parent=normal text}
\setbeamerfont*{author in head/foot}{parent=footline, series=\bfseries}

\setbeamercolor*{block title}{parent=palette secondary, use=palette secondary, bg=palette secondary.bg!70} 
\setbeamercolor*{block body}{parent=palette tertiary, use=palette tertiary, bg=palette tertiary.bg!50}

\setbeamercolor*{footline}{use=palette tertiary, bg=palette tertiary.bg!50}
\setbeamercolor*{titlelike}{parent=palette primary}
\setbeamercolor*{title}{parent=titlelike}

\setbeamercolor*{itemize item}{use=palette quaternary, fg=palette quaternary.bg}
\setbeamercolor*{itemize subitem}{use=palette tertiary, fg=palette tertiary.bg!50!black}
\setbeamercolor*{itemize subsubitem}{parent=itemize subitem}

\setbeamercolor*{enumerate item}{parent=itemize item}
\setbeamercolor*{enumerate subitem}{parent=itemize subitem}
\setbeamercolor*{enumerate subsubitem}{parent=itemize subsubitem}


\mode<all>
